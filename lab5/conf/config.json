{
  "redirect_rule":[{
      "to_uri":"/verynginx/index.html",
      "matcher":"demo_other_verynginx_uri",
      "enable":true
    }],
  "filter_rule":[{
      "enable":true,
      "code":"403",
      "response":"wp_Username_Enumeration_fix",
      "matcher":"wp_Username_Enumeration_fix",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "matcher":"localhost",
      "action":"accept"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_sql_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_backup_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_scan_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_code_0"
    },{
      "enable":false,
      "code":"403",
      "response":"dvwa_white_list",
      "matcher":"dvwa_white_list",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"403",
      "response":"sql_injection",
      "action":"block",
      "custom_response":true,
      "matcher":"sql_injection"
    },{
      "enable":true,
      "code":"403",
      "response":"vn_white_list",
      "matcher":"vn_white_list",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"403",
      "matcher":"prohibit_curl",
      "custom_response":false,
      "action":"block"
    }],
  "config_version":"0.36",
  "static_file_rule":[],
  "proxy_pass_rule":[{
      "enable":true,
      "matcher":"wordpress",
      "upstream":"wordpress_http",
      "proxy_host":""
    },{
      "enable":true,
      "matcher":"dvwa",
      "upstream":"dvwa",
      "proxy_host":""
    },{
      "enable":true,
      "matcher":"wordpress",
      "upstream":"wordpress_https",
      "proxy_host":""
    },{
      "enable":true,
      "matcher":"vn",
      "upstream":"vn",
      "proxy_host":""
    }],
  "summary_collect_rule":[],
  "summary_group_persistent_enable":true,
  "admin":[{
      "user":"verynginx",
      "password":"verynginx",
      "enable":true
    }],
  "dashboard_host":"",
  "response":{
    "limit_speed":{
      "content_type":"text/html",
      "body":"Your speed is too fast！！！！！！！！！！！！！！！"
    },
    "Ip_restriction":{
      "content_type":"text/html",
      "body":"该页面禁止通过ip地址访问，略略略~"
    },
    "demo_response_json":{
      "content_type":"application/json",
      "body":"{\"msg\":\"soms text\",\"status\":\"success\"}"
    },
    "vn_white_list":{
      "content_type":"text/html",
      "body":"你的IP不允许访问verynginx哦，略略略~"
    },
    "sql_injection":{
      "content_type":"text/html",
      "body":"No sql injection!!!!!!!!!!!!!!!!!!!!!"
    },
    "demo_response_html":{
      "content_type":"text/html",
      "body":"This is a html demo response"
    },
    "wp_Username_Enumeration_fix":{
      "content_type":"text/html",
      "body":"该文件禁止访问！"
    },
    "dvwa_white_list":{
      "content_type":"text/html",
      "body":"你的IP不允许访问哦，略略略~"
    }
  },
  "scheme_lock_enable":false,
  "uri_rewrite_rule":[{
      "to_uri":"/verynginx/$1",
      "matcher":"demo_verynginx_short_uri",
      "replace_re":"^/vn/(.*)",
      "enable":true
    }],
  "cookie_prefix":"verynginx",
  "filter_enable":true,
  "browser_verify_enable":true,
  "scheme_lock_rule":[{
      "enable":false,
      "matcher":"verynginx",
      "scheme":"https"
    }],
  "redirect_enable":true,
  "backend_upstream":{
    "vn":{
      "node":{
        "vn":{
          "host":"192.168.227.3",
          "weight":"1",
          "scheme":"http",
          "port":"80"
        }
      },
      "method":"random"
    },
    "dvwa":{
      "node":{
        "dvwa":{
          "host":"192.168.227.3",
          "weight":"1",
          "scheme":"http",
          "port":"8080"
        }
      },
      "method":"random"
    },
    "wordpress_https":{
      "node":{
        "wordpress_https":{
          "host":"192.168.227.3",
          "weight":"1",
          "scheme":"https",
          "port":"443"
        }
      },
      "method":"random"
    },
    "wordpress_http":{
      "node":{
        "wordpress_http":{
          "host":"192.168.227.3",
          "weight":"1",
          "scheme":"http",
          "port":"8082"
        }
      },
      "method":"random"
    }
  },
  "frequency_limit_enable":true,
  "matcher":{
    "demo_verynginx_short_uri":{
      "URI":{
        "operator":"≈",
        "value":"^/vn"
      }
    },
    "prohibit_curl":{
      "UserAgent":{
        "value":"curl",
        "operator":"≈"
      }
    },
    "sql_injection":{
      "Args":{
        "name_operator":"=",
        "name_value":"id",
        "operator":"≈",
        "value":"(select|union|order|or|and)"
      }
    },
    "localhost":{
      "IP":{
        "value":"192.168.227.3",
        "operator":"="
      }
    },
    "Ip_restriction":{
      "Host":{
        "value":"\\d+\\.\\d+\\.\\d+\\.\\d+",
        "operator":"≈"
      }
    },
    "demo_other_verynginx_uri":{
      "URI":{
        "operator":"=",
        "value":"/redirect_to_verynginx"
      }
    },
    "wordpress":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "attack_scan_0":{
      "UserAgent":{
        "operator":"≈",
        "value":"(nmap|w3af|netsparker|nikto|fimap|wget)"
      }
    },
    "attack_backup_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(htaccess|bash_history|ssh|sql)$"
      }
    },
    "dvwa_white_list":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      },
      "IP":{
        "value":"192.168.67.2",
        "operator":"!="
      }
    },
    "dvwa":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "verynginx":{
      "URI":{
        "operator":"≈",
        "value":"^/verynginx/"
      }
    },
    "attack_sql_0":{
      "Args":{
        "name_operator":"*",
        "operator":"≈",
        "value":"select.*from"
      }
    },
    "attack_code_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(git|svn|\\.)"
      }
    },
    "vn":{
      "URI":{
        "value":"vn.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "wp_Username_Enumeration_fix":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"="
      },
      "URI":{
        "value":"/wp-json/wp/v2/users",
        "operator":"="
      }
    },
    "vn_white_list":{
      "Host":{
        "value":"vn.sec.cuc.edu.cn",
        "operator":"="
      },
      "IP":{
        "value":"192.168.227.1",
        "operator":"!="
      }
    }
  },
  "static_file_enable":false,
  "summary_temporary_period":60,
  "summary_group_temporary_enable":true,
  "summary_with_host":false,
  "frequency_limit_rule":[{
      "enable":true,
      "separate":["ip"],
      "code":"403",
      "response":"limit_speed",
      "count":"50",
      "matcher":"dvwa",
      "custom_response":true,
      "time":"1"
    },{
      "enable":true,
      "separate":["ip"],
      "code":"403",
      "response":"limit_speed",
      "count":"20",
      "matcher":"wordpress",
      "custom_response":true,
      "time":"1"
    }],
  "uri_rewrite_enable":true,
  "base_uri":"/verynginx",
  "summary_request_enable":true,
  "proxy_pass_enable":true,
  "readonly":false,
  "browser_verify_rule":[]
}